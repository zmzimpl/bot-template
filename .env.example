# Application Environment
NODE_ENV=development

# API Configuration
API_BASE_URL=http://localhost:3000/api
API_TIMEOUT=30000

# HTTP Client Configuration
# Options: impit, got
HTTP_CLIENT=impit
HTTP2_ENABLED=true
KEEP_ALIVE=true
DNS_CACHE=true

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# Bark Push Notification Configuration
BARK_ENABLED=false
BARK_SERVER_URL=https://api.day.app
BARK_DEVICE_KEY=your-device-key
BARK_SOUND=bell
BARK_LEVEL=active
BARK_GROUP=Notifications
BARK_ICON=

# Encryption Keys
ENCRYPT_KEY1=your-encryption-key-1
ENCRYPT_KEY2=your-encryption-key-2

# Concurrency Control
MAX_CONCURRENCY=10
INTERVAL_MS=100

# Logging
LOG_LEVEL=info
